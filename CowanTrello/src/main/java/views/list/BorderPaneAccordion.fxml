<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <VBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="addLabelButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newLabelButton_OnClick" text="Add Label">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button id="addComponentButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newComponentButton_OnClick" text="Add Component">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button id="addMemberButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newMemberButton_OnClick" text="Add Member">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </VBox>
   </bottom>
   <center>
      <VBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label id="labelsLabel" text="Labels">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </Label>
            <ListView id="labelsListView" prefHeight="75.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </ListView>
            <Label id="componentsLabel" text="Components">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </Label>
            <ListView id="componentsListView" prefHeight="75.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </ListView>
            <Label id="membersLabel" text="Members">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </Label>
            <ListView id="membersListView" prefHeight="75.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </ListView>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
