<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="newViewVBox" alignment="CENTER" minWidth="250.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="createNewTitleLabel" text="Create a New List">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField id="createNewTextField" fx:id="newNameTextField" promptText="Name" />
      <Button id="createNewSubmitButton" fx:id="createNewSubmitButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#newCreate_OnClick" text="Create">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
